[supervisord]
nodaemon=true

[program:rails]
command=bundle exec rails server
autorestart=true

[program:gotty]
command=bundle exec rails gotty:server
autorestart=true
stdout_logfile=./log/gotty.log
stdout_logfile_maxbytes=1MB
stdout_logfile_backups=5
stdout_capture_maxbytes=1MB
redirect_stderr=true
